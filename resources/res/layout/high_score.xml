<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/home_page_dialog_id" android:background="@drawable/result_high_score" android:layout_width="match_parent" android:layout_height="match_parent">
